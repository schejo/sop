                        <label class="mylabel" value="TM PLANIFICADAS" style="font-size: 15px;"/>
                        <doublebox class="form-control" id="tmplanificadas"  constraint = "no negative" style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>

                    </n:div>
                    
                    <n:div class="form-group col-lg-5 col-md-5 mydiv1">
                        <label class="mylabel" value="PRODUCTO" style="font-size: 15px;"/>
                        <combobox id="producto" buttonVisible="true" width="280px"   model="${alltipoprod}" style="font-weight: bold; color:black; heigh:25px;"
                                  autodrop="true" >
                            <template name="model">
                                <comboitem label="${each.nombreAct}" value="${each.numAct}"/>
                            </template>
                        </combobox>
                    </n:div>
                    
                </n:div>
                <n:div class="row ">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    
                    <n:div class="form-group col-lg-5 col-md-5 mydiv1">
                        <label class="mylabel" value="DIRECTA" style="font-size: 15px;"/>
