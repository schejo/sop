                        <doublebox class="form-control" id="directa"   style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>

                    </n:div>
                    
                    <n:div class="form-group col-lg-5 col-md-5 mydiv1">
