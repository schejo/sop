    <div class="col-lg-12 col-md-12 col-sm-12" apply="ctrl.MantenimientoPlanificacionCtrl" style="">
        <n:h1>MANTENIMIENTO DE PLANIFICACION</n:h1>
        
        <n:div class="container " >
            
            <n:div id="dv1" class="">
                
                <n:div  class="row ">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1"></n:div>
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1"></n:div>
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <button  id="btnNuevo" label="" type="button" iconSclass="z-icon-plus-circle" class="btn btn btn-primary btn-lg" style="float: right; margin-top:9px; margin-left:10px;" />
                    </n:div>
                </n:div>
                
                <n:div class="row">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    <n:div class="form-group col-lg-3 col-md-3 ">
                        <label>Numero de DUA</label>

                    <div class="input-group">
                      <textbox id="dua" value="" class="form-control"/>
                      <span class="input-group-btn">
                        <button id="btnBusca1" class="btn btn-primary" type="button" iconSclass="z-icon-search" > 
                        </button>
                      </span>
                    </div>
                    </n:div>
                    <!--
                    <n:div class="form-group col-lg-7 col-md-7 mydiv1">
                        <label class="" value="Generado por:" style=""/>
                        <textbox class="form-control" id="usuario" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                    -->
                </n:div>
                
                <n:div class="row">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <label class="" value="Planififcacon" style=""/>
                        <textbox class="form-control" id="planificacion" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                    
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
