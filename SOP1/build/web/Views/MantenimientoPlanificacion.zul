                        <textbox class="form-control" id="tipo" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                </n:div>
                
                <n:div class="row">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    <!--
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <label class="" value="Fecha" style=""/>
                        <datebox id="fecha" width="100%" format="dd/MM/yyyy" locale="es" onCreate="self.value = new Date()" style="font-weight: bold; color:BLACK;"/>
                    </n:div>
                    -->
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <label class="" value="Estado" style=""/>
                        <textbox class="form-control" id="estado" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                    
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <label class="" value="Mes" style=""/>
                        <textbox class="form-control" id="mes" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                    
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <label class="" value="Entidad Retiene" style=""/>
                        <textbox class="form-control" id="retiene" value=""  style="font-weight: bold; font-size: 15px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                </n:div>
                
                <n:div  class="row ">
