                    </n:div>
                    
                    
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <button  id="btnActualizar" label="" type="button" iconSclass="z-icon-save" class="btn btn btn-success btn-lg" style="float: left; margin-top:9px;" />
                    </n:div>
                    <n:div class="form-group col-lg-3 col-md-3 mydiv1">
                        <button  id="btnEliminar" label="" type="button" iconSclass="z-icon-trash-o" class="btn btn btn-danger btn-lg" style="float: left; margin-top:9px; margin-left:10px;" />
                    </n:div>
                </n:div>
                
               
                
            </n:div>
            
        </n:div>
        
