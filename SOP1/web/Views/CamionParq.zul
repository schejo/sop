    <div class="col-lg-12 col-md-12 col-sm-12" apply="ctrl.CamionParqCtrl" style="">
        
        
        <n:h1>REVISION DE CAMION</n:h1>
        <n:div class="container " style="color:blue;" >
            <n:div class="myform">
                                
                <n:div class="row">
                    <n:div class="form-group col-lg-1 col-md-1 "></n:div>
                    
                    <n:div class="form-group col-lg-5 col-md-5 mydiv1">
                        <label class="mylabel" value="PLACA" style=""/>
                        <textbox class="form-control" id="placa" value=""  style="font-weight: bold; font-size: 20px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                    
                    <n:div class="form-group col-lg-5 col-md-5 mydiv1">
                        <label class="mylabel" value="CODIGO PAIS" style=""/>
                        <textbox class="form-control" id="cod_pais" value=""  style="font-weight: bold; font-size: 20px; margin-left:0px; heigh:20px; color:black; text-transform:uppercase; "/>
                    </n:div>
                                        
                </n:div>
            
                <n:div class="row">
