                        <combobox id="producto" buttonVisible="true" width="280px" autocomplete="true"  model="${alltipoact}" style="font-weight: bold; color:black; heigh:25px;"
                              autodrop="true" >
                        <template name="model">
                            <comboitem label="${each.nombreAct}" value="${each.numAct}"/>
                        </template>
                    </combobox>
                    </n:div>
                    
                </n:div>
